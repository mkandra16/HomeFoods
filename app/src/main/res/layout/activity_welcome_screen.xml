<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".WelcomeScreen">

    <Button
        android:id="@+id/bDishDescScreenNavigate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="69dp"
        android:text="@string/bDishDescScreenNavigation" />

    <Button
        android:id="@+id/bCheckoutScreenNavigate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bDishDescScreenNavigate"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="37dp"
        android:text="@string/bCheckoutScreenNavigate" />

    <Button
        android:id="@+id/bChefDeliveryScreenNavigate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bCheckoutScreenNavigate"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="37dp"
        android:text="@string/bChefDeliveryScreenNavigate" />

    <Button
        android:id="@+id/bChefMenuScreenNavigate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bChefDeliveryScreenNavigate"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="@string/bChefMenuScreenNavigate" />

    <Button
        android:id="@+id/bChefDishEditScreenNavigate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bChefMenuScreenNavigate"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="@string/bChefDishEditScreenNavigate" />

    <Button
        android:id="@+id/bFoodieResultsScreenNavigate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bChefDishEditScreenNavigate"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="@string/bFoodieResultsScreenNavigate" />

    <Button
        android:id="@+id/bChefHomePageScreenNavigate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bFoodieResultsScreenNavigate"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="@string/bChefHomePageScreenNavigate" />

</RelativeLayout>
