<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".WelcomeScreen">

    <include
        android:id="@+id/toolBar"
        layout="@layout/toolbar" />

    <Button
        android:id="@+id/bHomePageScreenNavigate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="60dp"
        android:text="@string/bHomePageScreenNavigate" />
    <Button
        android:id="@+id/bRegisterFoodie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bHomePageScreenNavigate"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="@string/bRegister" />
    <Button
        android:id="@+id/bSignInFoodie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bRegisterFoodie"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="@string/bSignIn" />
    <Button
        android:id="@+id/bTester"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bSignInFoodie"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="Test" />
</RelativeLayout>
